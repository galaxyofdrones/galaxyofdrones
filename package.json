    "leaflet": "^1.4",
    "leaflet-ajax": "^2.1",
    "lodash": "^4.17",
    "moment": "^2.23",
    "perfect-scrollbar": "^1.4",
    "pixi.js": "^4.8",
    "sass": "^1.16",
    "sass-loader": "^7.1",
    "socket.io-client": "^2.2",
